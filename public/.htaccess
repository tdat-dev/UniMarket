<IfModule mod_rewrite.c>
    Options -MultiViews
    RewriteEngine On

    # Chặn truy cập vào thư mục, file không tồn tại
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f

    # Chuyển tất cả request về index.php
    RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule>